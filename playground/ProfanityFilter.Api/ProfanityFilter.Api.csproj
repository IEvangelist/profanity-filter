<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>$(DefaultTargetFramework)</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>82eaa4db-aed7-4828-8fec-bb9d542765f4</UserSecretsId>
    <IsAotCompatible>false</IsAotCompatible>
    <GenerateDocumentationFile>false</GenerateDocumentationFile>
    <NoWarn>IDE0005;$(NoWarn)</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.OpenApi" />
    <PackageReference Include="Swashbuckle.AspNetCore" />
  </ItemGroup>

  <ItemGroup>

    <PackageReference Include="Microsoft.AspNetCore.SignalR.Client" VersionOverride="9.0.7" />

    <PackageReference Include="Microsoft.Extensions.Configuration.Binder" VersionOverride="9.0.7" />

    <PackageReference Include="Microsoft.Extensions.Diagnostics.HealthChecks.Abstractions" VersionOverride="9.0.7" />

    <PackageReference Include="Microsoft.Extensions.Hosting.Abstractions" VersionOverride="9.0.7" />

    <PackageReference Include="Microsoft.Extensions.Http" VersionOverride="9.0.7" />
    <ProjectReference Include="..\..\src\ProfanityFilter.Client\ProfanityFilter.Client.csproj" />
  </ItemGroup>

</Project>
